#======================================
  set rng_seed -6
  set NeighSkin 2.0
  set moves 600
  set cycles 10000
  SamplingType Umbrella
#  SamplingType Metropolis
#  SamplingType Min

#  forcefield testfield.txt
  forcefield "IonPair.clFF"

  Create boxes
    FromFile "PairConfig.clssy"
  End_Create

  neighlist 1 rsqlist 1

  modify Sampling biasvariables 1
  modify Sampling analysis 2

  Create moves
    atomtranslation 1.0
  End_Create

  Create analysis
    DistPair 1 2
  End_Create

  Create trajectory
    xyz 1 1000 "Traj.xyz"
  End_Create

  modify box 1 energycalc 1
  modify box 1 temperature 0.8
#=====================================
